<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="comp"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="/libs/granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <details
                                jcr:primaryType="nt:unstructured"
                                jcr:title="details"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <name
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="fullname"
                                        name="./fullname"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        fieldLabel="image"
                                        name="./image"/>
                                    <date
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="date"
                                        name="./date"/>
                                    <gender
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="gender"
                                        name="./gender">
                                        <items jcr:primaryType="nt:unstructured">
                                            <select
                                                jcr:primaryType="nt:unstructured"
                                                text="select"
                                                value="select"/>
                                            <male
                                                jcr:primaryType="nt:unstructured"
                                                text="male"
                                                value="male"/>
                                            <female
                                                jcr:primaryType="nt:unstructured"
                                                text="female"
                                                value="female"/>
                                        </items>
                                    </gender>
                                    <mobile_x0020_number
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                                        fieldLabel="mobilenumber"
                                        name="./mobilenumber"/>
                                    <checkbox
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}true"
                                        name="./checkbox"
                                        text="checkbox"/>
                                </items>
                            </details>
                            <multifield
                                jcr:primaryType="nt:unstructured"
                                jcr:title="multifield"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./multifield">
                                    <items jcr:primaryType="nt:unstructured">
                                        <Name
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Name"
                                            name="./name"/>
                                        <Date
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                            fieldLabel="Date"
                                            name="./date"/>
                                        <nestedmulti
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="nestedMulti">
                                            <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                name="./nestedMulti">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <number
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                        fieldLabel="number"
                                                        name="./number"/>
                                                    <address
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldLabel="address"
                                                        name="./address"/>
                                                </items>
                                            </field>
                                        </nestedmulti>
                                    </items>
                                </field>
                            </multifield>
                        </items>
                    </tabs>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
